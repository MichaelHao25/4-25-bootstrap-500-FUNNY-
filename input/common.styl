@import url(reset.css)

@font-face
	font-family 'Mina-Regular'
	font-display block
	src: url('../font/Mina-Regular.eot'); 
	src: url('../font/Mina-Regular.eot?#iefix') format('embedded-opentype'), 
		url('../font/Mina-Regular.woff') format('woff'), 
		url('../font/Mina-Regular.ttf') format('truetype'), 
		url('../font/Mina-Regular.svg#fontname') format('svg'); 
@font-face
	font-family 'Oxygen-Regular'
	font-display block
