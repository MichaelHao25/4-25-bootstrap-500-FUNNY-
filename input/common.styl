.header
	.brand
		padding-left 85px
		transition 0.3s
		@media(max-width 1000px)
			padding-left 0
			height 80px
		img
			height 100%
	.w1300
		@media(max-width 1300px)
			width 100%
	.nav
		padding-top 80px
		font-family 'Oxygen-Regular'
		line-height 52px
		font-size 20px
		color #3e85aa
		transition 0.3s
		@media(max-width 1300px)
			padding-top 0
			width 100%
			text-align center
			display -webkit-flex
			display -moz-flex
			display -ms-flex
			display -o-flex
			display flex
			overflow-x scroll
			justify-content center
			&::-webkit-scrollbar
				display none
		@media(max-width 800px)
			justify-content flex-start
		a
			float left
			margin-right 22px
			transition 0.3s
			@media(max-width 1300px)
				float none
			&:hover
			&.active
				font-weight bold
				color #fff
	.country-selection
		padding-top 80px
		margin 0 20px 0 100px
		position relative
		transition 0.3s
		@media(max-width 1300px)
			position absolute
			right 100px
			top 0
		@media(max-width 1000px)
			right 0
			padding-top 14px
			margin 0 15px 0 0
		.item
			position relative
			padding-left 70px
			padding-right 30px
			height 52px
			line-height 52px
			font-family 'Oxygen-Regular'
			font-weight lighter
			font-size 20px
			color #fff
			background url() left center no-repeat
			cursor pointer
			&:after
				content ""
				position absolute
				right 0
				top 0
				bottom 0
				width 15px
				background url('../img/Layer5.png') right center/13px auto no-repeat
		.option
			position absolute
			top 80px+52px
			left 0
			right 0
			background #000
			display none
			span
				display block
				padding-right 30px
				margin 5px 0
				text-align right
				font-size 20px
				color #fff
				text-transform uppercase
				line-height 1.5
				font-weight lighter
				cursor pointer


